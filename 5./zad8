#include <stdio.h>

void f(int tab[])
{
    int i,z,y;
    for (i=0;i<10;i++)
    {
        scanf("%d", &tab[i]);
        if (i>0)
        {
            if (tab[i-1]==tab[i])
                {
                printf("%d\n", tab[i]);
                printf("Podaj inna liczbe\n");
                }
        }
    }
}


int main()
{
    int tab[10];
    f(tab);
    
    
    return 0;
}
