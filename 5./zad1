#include <stdio.h>
#include <stdbool.h>

bool trojkat (float x, float y, float z)
{
  if (x+y>z && x+z>y && y+z>x)
        return true;
  else return false;
}


int main()
{
    float x,y,z;
    scanf("%f", &x);
    scanf("%f", &y);
    scanf("%f", &z);
    printf("%d", trojkat(x,y,z));

    return 0;
}
