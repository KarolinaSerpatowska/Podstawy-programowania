#include <stdio.h>
#include <math.h>

int main()
{
    int x1,x2,y1,y2;
    float pole;
    scanf("%d", &x1);
    scanf("%d", &y1);
    scanf("%d", &x2);
    scanf("%d", &y2);

    if (x2>x1 && y2>y1)
    {
        pole=1.0*(sqrt((x2-x1)*(x2-x1)))*(sqrt((y2-y1)*(y2-y1)));
    }
    else pole=1.0*(sqrt((x1-x2)*(x1-x2)))*(sqrt((y1-y2)*(y1-y2)));

    printf("%f", pole);

    return 0;
}
